{
  "tabWidth": 2,
  "semi": false,
  "printWidth": 80,
  "singleQuote": true,
  "endOfLine": "auto",
  "trailingComma": "none",
  "jsxSingleQuote": true,
  "arrowParens": "avoid",
  "bracketSpacing": true,
  "importOrderSeparation": true,
  "importOrderSortSpecifiers": false,
  "importOrder": [
    "<THIRD_PARTY_MODULES>",
    "^./types$|^./styles|^./logic$",
    "^./|^../",
    "^@components/atoms",
    "^@components/molecules",
    "^@components/organisms",
    "^@components/providers",
    "^@components/templates",
    "^@screens",
    "^@hooks",
    "^@store",
    "^@styles",
    "^@tests",
    "^@typescript",
    "^@utils",
    "^@routes",
    "^@assets",
    "^@config",
    "^@common"
  ]
}
